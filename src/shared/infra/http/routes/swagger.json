              "in": "path",
              "name": "id",
              "type": "string",
              "required": true
            }
          ],
          "responses": {
            "200": {}
          }
        },
        "delete": {
          "parameters": [
            {
              "in": "path",
              "name": "id",
              "type": "string",
              "required": true
            }
          ],
          "responses": {
