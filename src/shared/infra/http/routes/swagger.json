                    "emailDepartamento": {
                      "type": "string"
                    },
                    "status": {
                      "type": "string"
                    },
                    "dataIngresso": {
                      "type": "string",
                      "format": "date"
                    }
                  }
                }
              }
            },
            "responses": {
